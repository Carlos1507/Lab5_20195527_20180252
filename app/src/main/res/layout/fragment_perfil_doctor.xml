<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout3"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Fragments.PerfilDoctorFragment">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical" >
            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <TextView
                    android:id="@+id/titlePerfilDoctor"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/averiasanslibreregular"
                    android:textStyle="bold"
                    android:textSize="26sp"
                    android:textColor="#198eb6"
                    android:letterSpacing="-0.02"
                    android:lineSpacingExtra="0sp"
                    android:layout_marginTop="29dp"
                    android:gravity="center_horizontal"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    android:text="Perfil Doctor"/>

                <ImageView
                    android:id="@+id/regresar"
                    android:layout_width="26.7dp"
                    android:layout_height="26.7dp"
                    android:src="@drawable/baseline_arrow_back_ios_24"
                    android:clickable="true"
                    android:layout_marginStart="40dp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="@id/titlePerfilDoctor"
                    app:layout_constraintBottom_toBottomOf="@id/titlePerfilDoctor"/>

                <ImageView
                    android:id="@+id/fotoDoctor"
                    android:layout_width="138dp"
                    android:layout_height="138dp"
                    android:layout_marginTop="50dp"
                    app:layout_constraintStart_toStartOf="@id/regresar"
                    app:layout_constraintTop_toBottomOf="@id/titlePerfilDoctor"
                    tools:src="@tools:sample/avatars"
                    />
                <TextView
                    android:id="@+id/nombreDoctor"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/averiasanslibreregular"
                    android:textStyle="bold"
                    android:textSize="20sp"
                    android:textColor="#000000"
                    android:letterSpacing="-0.02"
                    android:lineSpacingExtra="0sp"
                    android:layout_marginTop="12dp"
                    android:layout_marginStart="24dp"
                    android:gravity="center_horizontal"
                    tools:text="Dr. Monsieur Juarez"
                    app:layout_constraintStart_toEndOf="@id/fotoDoctor"
                    app:layout_constraintTop_toTopOf="@id/fotoDoctor"/>
                <TextView
                    android:id="@+id/genero"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/averiasanslibreregular"
                    android:textStyle="normal"
                    android:textSize="17sp"
                    android:layout_marginStart="24dp"
                    android:textColor="#000000"
                    android:layout_marginTop="15dp"
                    android:letterSpacing="-0.02"
                    android:lineSpacingExtra="0sp"
                    app:layout_constraintStart_toEndOf="@id/fotoDoctor"
                    app:layout_constraintTop_toBottomOf="@id/nombreDoctor"
                    tools:text="Hombre"/>
                <TextView
                    android:id="@+id/correo"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/averiasanslibreregular"
                    android:textSize="10sp"
                    android:layout_marginTop="10dp"
                    android:textColor="#000000"
                    android:layout_marginStart="24dp"
                    android:letterSpacing="-0.02"
                    android:lineSpacingExtra="0sp"
                    app:layout_constraintStart_toEndOf="@id/fotoDoctor"
                    app:layout_constraintTop_toBottomOf="@id/genero"
                    tools:text="mailto:etienne.nguyen@example.com"/>
                <TextView
                    android:id="@+id/username"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/averiasanslibreregular"
                    android:textStyle="normal"
                    android:textSize="17sp"
                    android:textColor="#000000"
                    android:layout_marginTop="5dp"
                    android:letterSpacing="-0.02"
                    android:lineSpacingExtra="0sp"
                    app:layout_constraintStart_toStartOf="@id/fotoDoctor"
                    app:layout_constraintEnd_toEndOf="@id/fotoDoctor"
                    app:layout_constraintTop_toBottomOf="@id/fotoDoctor"
                    tools:text="angryfish444"/>
                <TextView
                    android:id="@+id/costolabel"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/averiasanslibreregular"
                    android:textStyle="normal"
                    android:textSize="17sp"
                    android:textColor="#000000"
                    android:letterSpacing="-0.02"
                    android:lineSpacingExtra="0sp"
                    android:layout_marginStart="24dp"
                    android:layout_marginTop="28dp"
                    app:layout_constraintTop_toBottomOf="@id/correo"
                    app:layout_constraintStart_toEndOf="@id/fotoDoctor"
                    android:text="Costo"/>
                <TextView
                    android:id="@+id/costo"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="20.1dp"
                    android:fontFamily="@font/averiasanslibreregular"
                    app:layout_constraintTop_toTopOf="@id/costolabel"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintBottom_toBottomOf="@id/costolabel"
                    android:textStyle="normal"
                    android:textSize="17sp"
                    android:textColor="#000000"
                    android:gravity="center_horizontal"
                    tools:text="S/ 165.00"/>
                <TextView
                    android:id="@+id/labelUbicacion"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/averiasanslibreregular"
                    android:textStyle="normal"
                    android:textSize="22sp"
                    android:textColor="#198eb6"
                    android:letterSpacing="-0.02"
                    android:layout_marginStart="40dp"
                    android:layout_marginTop="47dp"
                    android:lineSpacingExtra="0sp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/username"
                    android:text="Ubicación"/>
                <TextView
                    android:id="@+id/labelEdad"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/averiasanslibreregular"
                    android:textStyle="normal"
                    android:textSize="22sp"
                    android:layout_marginTop="53dp"
                    android:layout_marginStart="40dp"
                    android:textColor="#198eb6"
                    android:letterSpacing="-0.02"
                    android:lineSpacingExtra="0sp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/labelUbicacion"
                    android:text="Edad"/>
                <TextView
                    android:id="@+id/labelTelefono"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/averiasanslibreregular"
                    android:textStyle="normal"
                    android:textSize="22sp"
                    android:layout_marginTop="53dp"
                    android:layout_marginStart="40dp"
                    android:textColor="#198eb6"
                    android:letterSpacing="-0.02"
                    android:lineSpacingExtra="0sp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/labelEdad"
                    android:text="Teléfono"/>
                <TextView
                    android:id="@+id/labelNacionalidad"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/averiasanslibreregular"
                    android:textStyle="normal"
                    android:textSize="22sp"
                    android:layout_marginTop="53dp"
                    android:layout_marginStart="40dp"
                    android:textColor="#198eb6"
                    android:letterSpacing="-0.02"
                    android:lineSpacingExtra="0sp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/labelTelefono"
                    android:text="Nacionalidad"/>
                <TextView
                    android:id="@+id/nacionalidad"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/averiasanslibreregular"
                    android:textStyle="normal"
                    android:textSize="15sp"
                    android:textColor="#000000"
                    android:letterSpacing="-0.02"
                    android:lineSpacingExtra="0sp"
                    android:gravity="center_horizontal"
                    android:layout_marginTop="12dp"
                    android:layout_marginBottom="30dp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/labelNacionalidad"
                    tools:text="AU"/>
                <TextView
                    android:id="@+id/telefono"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/averiasanslibreregular"
                    android:textStyle="normal"
                    android:textSize="15sp"
                    android:textColor="#000000"
                    android:letterSpacing="-0.02"
                    android:lineSpacingExtra="0sp"
                    android:gravity="center_horizontal"
                    android:layout_marginTop="12dp"
                    app:layout_constraintTop_toBottomOf="@id/labelTelefono"
                    tools:text="0472-492-054"/>
                <TextView
                    android:id="@+id/edad"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/averiasanslibreregular"
                    android:textStyle="normal"
                    android:textSize="15sp"
                    android:textColor="#000000"
                    android:letterSpacing="-0.02"
                    android:lineSpacingExtra="0sp"
                    android:gravity="center_horizontal"
                    android:layout_marginTop="12dp"
                    app:layout_constraintTop_toBottomOf="@id/labelEdad"
                    tools:text="50"/>
                <TextView
                    android:id="@+id/ubicacion"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/averiasanslibreregular"
                    android:textStyle="normal"
                    android:textSize="15sp"
                    android:textColor="#000000"
                    android:letterSpacing="-0.02"
                    android:lineSpacingExtra="0sp"
                    android:gravity="center_horizontal"
                    android:layout_marginTop="12dp"
                    app:layout_constraintTop_toBottomOf="@id/labelUbicacion"
                    tools:text="Australia - Northern Territory - Warrnambool"/>
            </androidx.constraintlayout.widget.ConstraintLayout>
        </LinearLayout>
    </ScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>